        "name": "define state",
        "func": "let cmd = msg.payload;\nif (cmd.includes(\"steer_front\")){\n    msg.payload = \"Frente\";\n    global.set(\"steering_motor_state\", \"Rodas: Frente\")\n}\nelse if (cmd.includes(\"steer_back\")) {\n    msg.payload = \"Trás\";\n    global.set(\"steering_motor_state\", \"Rodas: Trás\")\n}\nelse if (cmd.includes(\"steer_right\")) {\n    msg.payload = \"Direita\";\n    global.set(\"steering_motor_state\", \"Rodas: Direita\")\n}\nelse if (cmd.includes(\"steer_left\")) {\n    msg.payload = \"Esquerda\";\n    global.set(\"steering_motor_state\", \"Rodas: Esquerda\")\n}\nelse if (cmd.includes(\"steer_stop\")) {\n    msg.payload = \"Paradas\";\n    global.set(\"steering_motor_state\", \"Rodas: Paradas\")\n}\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "global.set(\"steering_motor_state\", \"Rodas: Paradas\")",
